<% include ../partials/header %>


    <div id="wrapper">

      <% include ../partials/nav_side %>


        <div id="page-wrapper" class="gray-bg">
        <div class="row border-bottom">

          <% include ../partials/nav_top %>

        </div>
        <div class="row wrapper border-bottom white-bg page-heading">
            <div class="col-lg-10">
                <h2>Employees</h2>
                <ol class="breadcrumb">
                    <li>
                        <a href="/employees/new">Add</a>
                    </li>
                </ol>
            </div>
            <div class="col-lg-2">

            </div>
        </div>
        <div class="wrapper wrapper-content animated fadeInRight">
        <div class="row">
          <% employees.forEach(function (employee) { %>
            <div class="col-lg-4">
                <div class="contact-box">
                    <a href="/employees/<%=employee.id%>">
                    <div class="col-sm-4">
                        <div class="text-center">
                            <img alt="image" class="img-circle m-t-xs img-responsive" src="<%=employee.avatar%>">
                            <div class="m-t-xs font-bold">Graphics designer</div>
                        </div>
                    </div>
                    <div class="col-sm-8">
                        <h3><strong><%=employee.first_name%> <%=employee.last_name%></strong></h3>
                        <p><i class="fa fa-map-marker"></i> Riviera State 32/106</p>
                        <address>
                            <strong>Twitter, Inc.</strong><br>
                            795 Folsom Ave, Suite 600<br>
                            San Francisco, CA 94107<br>
                            <abbr title="Phone">P:</abbr> (123) 456-7890
                        </address>
                    </div>
                    <div class="clearfix"></div>
                        </a>
                </div>
            </div>
            <% }) %>
        </div>
        </div>
        <div class="footer">
            <div class="pull-right">
                10GB of <strong>250GB</strong> Free.
            </div>
            <div>
                <strong>Copyright</strong> Example Company &copy; 2014-2017
            </div>
        </div>

        </div>
        </div>

        <% include ../partials/footer %>
